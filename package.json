{
	"name": "repository-find",
	"version": "1.0.0",
	"description": "Search github repositories",
	"author": "Nick Semu",
	"license": "ISC",
	"keywords": [
		"github, search, repository"
	],
	"scripts": {
		"lint": "eslint --fix './src/**/*.{js,jsx,ts,tsx}' --config ./.eslintrc.json",
		"format": "prettier --write './src/**/*.{js,jsx,ts,tsx,css,json}' --config ./.prettierrc.json",
		"start": "vite"
	},
	"lint-staged": {
		"./src/**/*.{js,jsx,ts,tsx}": [
			"prettier --config ./.prettierrc.json --write",
			"eslint --config ./.eslintrc.json --fix"
		]
	},
	"dependencies": {
		"@apollo/client": "^3.7.1",
		"@reduxjs/toolkit": "^1.9.5",
		"graphql": "^16.6.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-redux": "^8.0.5",
		"react-router-dom": "^6.4.3"
	},
	"devDependencies": {
		"@types/node": "^20.2.3",
		"@types/react": "^18.0.25",
		"@types/react-dom": "^18.2.4",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"@vitejs/plugin-react": "^2.1.0",
		"dotenv": "^16.0.3",
		"eslint": "^8.26.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsx-a11y": "^6.6.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.31.10",
		"eslint-plugin-react-hooks": "^4.6.0",
		"husky": "^8.0.0",
		"lint-staged": "^13.0.3",
		"prettier": "^2.7.1",
		"process": "^0.11.10",
		"typescript": "^4.8.4",
		"vite": "^4.3.2",
		"vite-plugin-svgr": "^2.2.1",
		"vite-tsconfig-paths": "^4.2.0"
	},
	"engines": {
		"node": ">=16"
	},
	"browserslist": "last 2 versions, not dead"
}
